<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ouyangwei.multimodule.dao.mappers.OrderMapper">
    <select id="getOrdersByUserId" resultType="com.ouyangwei.multimodule.dao.entities.Order">
        select * from `order` where user_id=#{userId};
